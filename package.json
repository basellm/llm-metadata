{
  "name": "llm-metadata",
  "version": "0.1.0",
  "private": true,
  "description": "High-throughput friendly, static-by-default interface: rebuild on change; serve static JSON via GitHub Pages.",
  "license": "MIT",
  "engines": {
    "node": ">=18.17"
  },
  "scripts": {
    "build": "node scripts/build.js",
    "build:force": "node scripts/build.js --force",
    "clean": "node -e \"require('fs').rmSync('.cache',{recursive:true,force:true});require('fs').rmSync('dist',{recursive:true,force:true});\"",
    "check": "node scripts/build.js --check"
  },
  "dependencies": {},
  "devDependencies": {}
}